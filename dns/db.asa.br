;
; BIND data file for local loopback interface
;
$TTL 604800
@   IN  SOA ns.asa.br. root.asa.br. (
                  2       ; Serial
             604800       ; Refresh
              86400       ; Retry
            2419200       ; Expire
             604800 )     ; Negative Cache TTL
;
@   IN  NS  ns.
@   IN  A   10.24.3.148
ns  IN  A   10.24.3.148

www IN  A   10.24.3.148
portal IN CNAME www
ana    IN CNAME www
mail   IN CNAME www #add
proxy  IN CNAME www #add



